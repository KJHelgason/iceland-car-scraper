# Copy this file to .env and fill in secrets
# Google Gemini API key
GOOGLE_API_KEY=replace_me

# Database URL (default uses SQLite in mounted data directory)
# For Postgres: postgresql://user:password@host:5432/database
DATABASE_URL=sqlite:///data/app.db

# Logging level: DEBUG | INFO | WARNING | ERROR
LOG_LEVEL=INFO

# Timezone for scheduler (e.g., Europe/Reykjavik)
TZ=UTC
